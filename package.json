{
  "name": "react-starter-secure-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "analyze": "npm run build && npx serve dist -s",
    "bundle-analyzer": "npm run build && npx serve dist -s -p 3000 & sleep 2 && open http://localhost:3000/stats.html",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "playwright test",
    "test:accessibility": "playwright test tests/accessibility-fixed.spec.ts --headed",
    "test:accessibility:headless": "playwright test tests/accessibility-fixed.spec.ts",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-select": "^2.2.6",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "dompurify": "^3.2.6",
    "lucide-react": "^0.542.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.34.0",
